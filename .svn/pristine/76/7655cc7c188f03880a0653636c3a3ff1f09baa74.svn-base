package cn.hrms.train.entity;

import java.io.Serializable;
import java.util.Date;

/*
 * 培训审核
 */
public class TrainAudit implements Serializable {

	private static final long serialVersionUID = 2406063211259764982L;
	
	/*
	 * 培训编号
	 */
	private Train train;
	
	/*
	 * 审核结果
	 */
	private String result;
	
	/*
	 * 审核意见
	 */
	private String opinion;
	
	/*
	 * 审核人
	 */
	private String person;
	
	/*
	 * 审核时间
	 */
	private Date time;

	public TrainAudit(Train train, String result, String opinion,
			String person, Date time) {
		super();
		this.train = train;
		this.result = result;
		this.opinion = opinion;
		this.person = person;
		this.time = time;
	}

	public TrainAudit() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Train getTrain() {
		return train;
	}

	public void setTrain(Train train) {
		this.train = train;
	}

	public String getResult() {
		return result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	public String getOpinion() {
		return opinion;
	}

	public void setOpinion(String opinion) {
		this.opinion = opinion;
	}

	public String getPerson() {
		return person;
	}

	public void setPerson(String person) {
		this.person = person;
	}

	public Date getTime() {
		return time;
	}

	public void setTime(Date time) {
		this.time = time;
	}
	
}
